<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:jsf="http://jsf.jd.com/schema/jsf"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	 http://jsf.jd.com/schema/jsf http://jsf.jd.com/schema/jsf/jsf.xsd" default-autowire="byName">

    <!-- 发送邮件服务 -->
    <jsf:consumer id="mailService" interface="com.jd.jr.service.notice.IMailService"
                  protocol="jsf" alias="mailServiceProvider" timeout="60000" retries="0">
    </jsf:consumer>
</beans>
